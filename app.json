{
  "expo": {
    "name": "Recorder",
    "description": "Share Your Record!",
    "slug": "recordShare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#01579b",
    "splash": {
      "image": "./assets/splash_mike.png",
      "resizeMode": "contain",
      "backgroundColor": "#01579b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "loading": {
      "hideExponentText": false,
      "icon": "./assets/icon.png"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.vermashivam.recordShare",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#01579b"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
